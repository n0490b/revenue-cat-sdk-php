{
    "name": "lanos/revenuecat-sdk-php",
    "description": "A PHP SDK for the RevenueCat API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lanos\\RevenueCat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lanos\\RevenueCat\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Rob Lane",
            "email": "rob@lanos.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}